<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:centerX="0.4"
                android:centerY="0.6"
                android:endColor="@color/btnStrokePrimaryDark"
                android:startColor="@color/btnStrokePrimary" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/dimen_1dp"
        android:left="@dimen/dimen_1dp"
        android:right="@dimen/dimen_1dp"
        android:top="@dimen/dimen_1dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:centerColor="@color/btnPrimaryNormal"
                android:centerX="0.6"
                android:endColor="@color/btnPrimary"
                android:startColor="@color/btnPrimaryDark" />
        </shape>
    </item>
</layer-list>